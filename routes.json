{
  "auth-token": {
    "handler": "functions/auth/token.post",
    "events": [
      {
        "http": {
          "method": "post",
          "path": "auth/token"
        }
      }
    ]
  },
  "graphql": {
    "handler": "functions/graphql.post",
    "events": [
      {
        "http": {
          "method": "post",
          "path": "_graphql"
        }
      }
    ]
  },
  "rest": {
    "handler": "functions/router.handler",
    "events": [
      {
        "http": {
          "method": "any",
          "path": "{path+}"
        }
      }
    ]
  }
}
